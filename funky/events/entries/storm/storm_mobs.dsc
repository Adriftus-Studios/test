storm_entity:
    type: entity
    entity_type: drowned
    mechanisms:
        custom_name: <aqua>Storm Entity
        custom_name_visible: true

storm_entity_disguise:
    type: world
    debug: false
    events:
        after storm_entity added to world:
            - disguise <context.entity> global as:<server.online_players.random.with_single[skin_;]>
